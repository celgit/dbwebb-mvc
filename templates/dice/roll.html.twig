{% extends "base.html.twig" %}

{% block title %}Dice{% endblock %}

{% block content %}
<h1>{{ title }}</h1>

<p>The dice is rolled {{ numRolls }} times.</p>

<div class="textHandle">
        <ul>
            {% for roll in rolls %}
                <li>{{ roll }}</li>
            {% endfor %}
        </ul>
</div>
{% endblock %}
